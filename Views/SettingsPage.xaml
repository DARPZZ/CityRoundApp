<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Vamdrup_rundt.Views.SettingsPage"
             Title="SettingsPage">
    <VerticalStackLayout Padding="0,60,0,0" >
        <Button Text="Logout"
                WidthRequest="200"
                FontSize="Large"
                Command="{Binding SignOutClickedCommand}"
                >
        </Button>
        <Button
                Margin="0,40,0,0"
                 Text="Go back to main page"
                 WidthRequest="200"
                 FontSize="Large"
                 Command="{Binding BackToMainClickedCommand}"
                 >
        </Button>

    </VerticalStackLayout>
</ContentPage>